    <EditText
        android:id="@+id/usuario"
        android:hint="Usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <EditText
        android:id="@+id/contrasena"
        android:hint="ContraseÃ±a"
        android:inputType="textPassword"
        android:layout_width="wrap_content"
